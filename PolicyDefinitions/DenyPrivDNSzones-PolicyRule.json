{
    "if": {
        "allOf": [
            {
                "equals": "Microsoft.Network/privateDnsZones",
                "field": "type"
            },
            {
                "count": {
                    "value": "[parameters('ExcludeDNSzones')]",
                    "where": {
                        "field": "name",
                        "like": "[current()]"
                    }
                },
                "equals": 0
            }
        ]
    },
    "then": {
        "effect": "[parameters('effect')]"
    }
}